{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/**/test/**/*.test.*"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [
      "@virtualpatterns/mablung-source-map-support/install"
    ],
    "verbose": true
  },
  "babel": {
    "presets": [
      "@virtualpatterns/babel-preset-mablung-makefile"
    ],
    "env": {
      "commonjs": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "c8": {
    "all": true,
    "exclude": [],
    "reporter": [
      "html"
    ],
    "src": "release"
  },
  "description": "TBD",
  "eslintConfig": {
    "extends": "@virtualpatterns/eslint-config-mablung-makefile"
  },
  "exports": {
    ".": "./release/index.cjs",
    "./package.json": "./package.json"
  },
  "files": [
    "release/index.cjs"
  ],
  "license": "GPL-3.0+",
  "name": "@virtualpatterns/eslint-config-mablung-makefile-environment",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/eslint-config-mablung-makefile-environment"
  },
  "type": "module",
  "version": "0.0.1-2",
  "dependencies": {
    "@pwn/is": "^1.0.2",
    "jsonpath": "^1.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "@virtualpatterns/babel-preset-mablung-makefile": "0.0.1-1",
    "@virtualpatterns/eslint-config-mablung-makefile": "0.0.1-12",
    "@virtualpatterns/mablung-makefile-environment": "0.0.1-28",
    "ava": "^3.15.0"
  },
  "peerDependencies": {
    "@virtualpatterns/mablung-makefile-environment": "0.0.1-28"
  }
}
